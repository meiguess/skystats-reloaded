      </div>
    <% } else {%>
    <% if (minecraftMaterials.includes(secondLegArray[j].material)) { %>
      <div class="wardrobe-set">
        <div class="piece piece-<%= loreToRarity(secondLegArray[j].lore); %>-bg <%= secondLegArray[j].name %>" lore="<%= secondLegArray[j].lore %>">
          <div class="piece-1-icon armor-piece" style="--item-icon: url('../Resources/armor/<%= secondLegArray[j].material.toLowerCase() %>.png')"></div>
        </div>
      </div>
    <% } else { %>
    <div class="wardrobe-set">
      <div class="piece piece-<%= loreToRarity(secondLegArray[j].lore); %>-bg <%= secondLegArray[j].name %>" lore="<%= secondLegArray[j].lore %>">
        <div class="piece-1-icon armor-piece" style="--item-icon: url('<%= armorAttr[secondLegArray[j].type.toLowerCase()] === undefined ? 'https://sky.lea.moe/leather/leggings/' + intToRGB(secondLegArray[j].color).r + ',' + intToRGB(secondLegArray[j].color).g + ',' + intToRGB(secondLegArray[j].color).b + '' : armorAttr[secondLegArray[j].type.toLowerCase()] %>');"></div>
      </div>
    </div>
    <% } } }%>
  </div>
  <!-- Boot -->
  <div class="row">
    <% for(let j = 0; j < secondBootArray.length; j++) { if(secondBootArray[j] === null) { %>
