    <% } else { %>
    <div class="wardrobe-set">
      <div class="piece piece-<%= loreToRarity(secondBootArray[j].lore); %>-bg <%= secondBootArray[j].name %>" lore="<%= secondBootArray[j].lore %>">
        <div class="piece-1-icon armor-piece" style="--item-icon: url('<%= armorAttr[secondBootArray[j].type.toLowerCase()] === undefined ? 'https://sky.lea.moe/leather/boots/' + intToRGB(secondBootArray[j].color).r + ',' + intToRGB(secondBootArray[j].color).g + ',' + intToRGB(secondBootArray[j].color).b + '' : armorAttr[secondBootArray[j].type.toLowerCase()] %>');"></div>
