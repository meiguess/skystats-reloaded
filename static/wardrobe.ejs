    <% } else if (secondHeadArray[j].material.toLowerCase() === "leather_helmet") { %>
      <div class="wardrobe-set">
        <div class="piece piece-<%= loreToRarity(secondHeadArray[j].lore); %>-bg <%= secondHeadArray[j].name %>" lore="<%= secondHeadArray[j].lore %>">
          <div class="piece-1-icon armor-piece" style="--item-icon: url('<%= 'https://sky.lea.moe/leather/helmet/' + intToRGB(secondHeadArray[j].color).r + ',' + intToRGB(secondHeadArray[j].color).g + ',' + intToRGB(secondHeadArray[j].color).b %>')"></div>
        </div>
      </div>
    <% } else { %>
    <div class="wardrobe-set">
      <div class="piece piece-<%= loreToRarity(secondHeadArray[j].lore); %>-bg <%= secondHeadArray[j].name %>" lore="<%= secondHeadArray[j].lore %>">
        <div class="piece-1-icon armor-piece" style="--item-icon: url('<%= secondHeadArray[j].texture === undefined ? armorAttr[secondHeadArray[j].type.toLowerCase()] : secondHeadArray[j].texture.replace(`http://textures.minecraft.net/texture/`, `https://mc-heads.net/head/`);  %>')"></div>
      </div>
    </div>
    <% } } }%>
  </div>
  <!-- Chest -->
  <div class="row">
    <% for(let j = 0; j < secondChestArray.length; j++) { if(secondChestArray[j] === null) { %>
      <div class="wardrobe-set">
        <div class="piece piece-NONE-bg"></div>
      </div>
    <% } else {%>
    <% if (minecraftMaterials.includes(secondChestArray[j].material)) { %>
      <div class="wardrobe-set">
        <div class="piece piece-<%= loreToRarity(secondChestArray[j].lore); %>-bg <%= secondChestArray[j].name %>" lore="<%= secondChestArray[j].lore %>">
          <div class="piece-1-icon armor-piece" style="--item-icon: url('../Resources/armor/<%= secondChestArray[j].material.toLowerCase() %>.png')"></div>
        </div>
      </div>
    <% } else { %>
    <div class="wardrobe-set">
      <div class="piece piece-<%= loreToRarity(secondChestArray[j].lore); %>-bg <%= secondChestArray[j].name %>" lore="<%= secondChestArray[j].lore %>">
        <div class="piece-1-icon armor-piece" style="--item-icon: url('<%= armorAttr[secondChestArray[j].type.toLowerCase()] === undefined ? 'https://sky.lea.moe/leather/chestplate/' + intToRGB(secondChestArray[j].color).r + ',' + intToRGB(secondChestArray[j].color).g + ',' + intToRGB(secondChestArray[j].color).b + '' : armorAttr[secondChestArray[j].type.toLowerCase()] %>');"></div>
      </div>
    </div>
